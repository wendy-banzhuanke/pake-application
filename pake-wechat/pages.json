{
	"easycom": {
		"autoscan": true,
		// 注意一定要放在custom里，否则无效，https://ask.dcloud.net.cn/question/131175
		"custom": {
			// "^tbh-(.*)": "@/components/tbh-$1/tbh-$1.vue",
			"^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
			"^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
	    "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue"
		}
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path": "pages/weather/index",
			"style": {
				"navigationBarTitleText": "天气"
			}
		},
		{
			"path": "pages/my/index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#333333",
		"fontSize": "12px",
		"height": "50px",
		"iconWidth": "24px",
		"spacing": "3px",
		"selectedColor": "#0066FF",
		"backgroundColor": "white",
		"list": [
			{
				"pagePath": "pages/index/index",
				"iconPath": "static/images/tab-bar/home.png",
				"selectedIconPath": "static/images/tab-bar/home-s.png",
				"text": "首页"
			},
			{
				"pagePath": "pages/my/index",
				"iconPath": "static/images/tab-bar/mine.png",
				"selectedIconPath": "static/images/tab-bar/mine-s.png",
				"text": "我的"
			}
		]
	},
	"uniIdRouter": {}
}
